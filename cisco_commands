		switchport mode [access|trunk]
		switchport access vlan <1-4094>
		switchport voice vlan X
	(config)vlan <1-4094>
	(config)vlan XX
		name _name

Trunk

	#show dtp interface int_name
	#show interface int_name switchport
	#show interface [int_name] trunk
	(config-if)switchport mode trunk desirable?
	(config-if)switchport trunk encapsulation/
		  	   /[dot1q|isl|negotiate]
	(config-if)switchport trunk native vlan X
	(config-if)switchport nonegotiate

		   switchport trunk allowed vlan [add|all/
			        /except|remove] vlan_list

